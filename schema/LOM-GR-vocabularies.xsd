            <enumeration value="χορηγός"/>
            <enumeration value="πιστοποιητής"/>
            <enumeration value="άγνωστος"/>
        </restriction>
    </simpleType>


    <simpleType name="metaRole">
        <restriction base="token">
            <!-- Photodentro DS AP extended values -->
            <enumeration value="μετασχολιαστής"/>
            <enumeration value="εμπλουτιστής"/>
            <enumeration value="πάροχος"/>
            <enumeration value="επικυρωτής"/>
        </restriction>
    </simpleType>

    <simpleType name="techReqType">
        <restriction base="token">
            <!-- Photodentro DS AP extended values -->
            <enumeration value="λειτουργικό σύστημα"/>
            <enumeration value="φυλλομετρητής"/>
            <enumeration value="πρόσθετα"/>
            <enumeration value="λογισμικό"/>
        </restriction>
    </simpleType>

    <simpleType name="techReqName">
        <restriction base="token">
            <!-- IEEE LOM default values -->
            <enumeration value="any"/>
            <enumeration value="netscape communicator"/>
            <enumeration value="ms-internet explorer"/>
            <enumeration value="opera"/>
            <enumeration value="amaya"/>
            <enumeration value="pc-dos"/>
            <enumeration value="ms-windows"/>
            <enumeration value="macos"/>
            <enumeration value="unix"/>
            <enumeration value="multi-os"/>
            <enumeration value="none"/>
            <!-- Photodentro DS AP extended values -->
            <enumeration value="linux"/>				<!-- os -->
            <enumeration value="mozilla firefox"/>		<!-- browsers -->
            <enumeration value="safari"/>
            <enumeration value="google chrome"/>
            <enumeration value="adobe flash player"/>	<!-- plugin -->
            <enumeration value="quickTime"/>
            <enumeration value="real player"/>
            <enumeration value="media player"/>
            <enumeration value="microsoft silverlight"/>
            <enumeration value="geogebra"/>
            <enumeration value="geogebra 3.2 libraries (by Photodentro)"/>
